				"wikimediaapiportaloauth-ui-error-request-client-fail",
				"wikimediaapiportaloauth-ui-field-secret",
				"wikimediaapiportaloauth-ui-client-details-reset-title",
				"wikimediaapiportaloauth-ui-action-done",
