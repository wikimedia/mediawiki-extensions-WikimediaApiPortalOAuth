		"AppManagement": "MediaWiki\\Extension\\WikimediaApiPortalOAuth\\Special\\AppManagement"
	},
	"config": {
		"WikimediaApiPortalOAuthMetaRestURL": {
			"value": "https://meta.wikimedia.org/w/rest.php",
			"description": "URL to rest.php on Meta"
		}
	},
