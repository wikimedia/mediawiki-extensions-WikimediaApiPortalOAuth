{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"mw": "readonly",
		"OO": "readonly"
	},
	"rules": {
		"no-prototype-builtins": "off",
		"no-jquery/no-global-selector": "off",
		"vars-on-top": "off",
		"one-var": "off",
		"mediawiki/class-doc": "off",
		"mediawiki/msg-doc": "off"
	}
}
